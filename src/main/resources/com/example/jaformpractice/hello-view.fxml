<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox alignment="CENTER" prefHeight="506.0" prefWidth="516.0" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/19" fx:controller="com.example.jaformpractice.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <Text fx:id="labelFistName" strokeType="OUTSIDE" strokeWidth="0.0" text=" First name" />
   <TextField fx:id="inputFirstName" />
   <Text fx:id="labelLastName" strokeType="OUTSIDE" strokeWidth="0.0" text="Last name" />
   <TextField fx:id="inputLastName" />
   <Text fx:id="labelGender" strokeType="OUTSIDE" strokeWidth="0.0" text="Gender" />
   <TextField fx:id="inputGender" />
   <Text fx:id="labelAge" strokeType="OUTSIDE" strokeWidth="0.0" text="Age" />
   <TextField fx:id="inputAge" />
    <Button onAction="#onButtonClick" text="Submit" />
</VBox>
